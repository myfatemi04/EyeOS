{
  "name": "eyeos-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "eyeos-electron/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "circular-std": "^1.0.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^13.6.6",
    "electron-builder": "^22.4.1",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0",
    "node-sass": "^4.13.1",
    "python-shell": "^1.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-history": "^0.18.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.7.5",
    "unstated": "^2.1.1",
    "wait-on": "^4.0.1"
  },
  "scripts": {
    "react-build:style": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "react-start": "npm run react-build:style && react-scripts start",
    "react-build": "npm run react-build:style && react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "postcss-cli": "^7.1.0"
  }
}
